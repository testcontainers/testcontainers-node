{
  "name": "testcontainers",
  "author": "Cristian Greco",
  "version": "9.4.0",
  "main": "dist/src/index",
  "types": "dist/src/index",
  "keywords": [
    "docker",
    "testcontainers"
  ],
  "files": [
    "dist"
  ],
  "description": "Testcontainers is a NodeJS library that supports tests, providing lightweight, throwaway instances of common databases, Selenium web browsers, or anything else that can run in a Docker container.",
  "license": "MIT",
  "homepage": "https://github.com/testcontainers/testcontainers-node#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/testcontainers/testcontainers-node"
  },
  "bugs": {
    "url": "https://github.com/testcontainers/testcontainers-node/issues"
  },
  "scripts": {
    "prepare": "npm run build && husky install",
    "pre-commit": "lint-staged",
    "build": "npm run clean && tsc -p tsconfig.prod.json",
    "docs:serve": "docker-compose up",
    "clean": "rimraf dist",
    "test": "cross-env DEBUG=testcontainers* jest",
    "test:ci": "npm run test -- --runInBand --coverage",
    "format": "prettier --write package.json \"src/**/*.ts\"",
    "lint": "eslint --fix package.json \"src/**/*.ts\"",
    "lint:ci": "eslint package.json \"src/**/*.ts\" --max-warnings=0",
    "updateDeps": "npx npm-check-updates -u",
    "validateEngine": "npx ls-engines"
  },
  "dependencies": {
    "@balena/dockerignore": "^1.0.2",
    "@types/archiver": "^5.3.2",
    "@types/dockerode": "^3.3.16",
    "archiver": "^5.3.1",
    "async-lock": "^1.4.0",
    "byline": "^5.0.0",
    "debug": "^4.3.4",
    "docker-compose": "^0.23.19",
    "dockerode": "^3.3.5",
    "get-port": "^5.1.1",
    "node-fetch": "^2.6.9",
    "properties-reader": "^2.2.0",
    "ssh-remote-port-forward": "^1.0.4",
    "tar-fs": "^2.1.1"
  },
  "devDependencies": {
    "@elastic/elasticsearch": "^7.17.0",
    "@types/async-lock": "^1.4.0",
    "@types/byline": "^4.2.33",
    "@types/debug": "4.1.7",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/node-fetch": "^2.6.3",
    "@types/pg": "^8.6.6",
    "@types/properties-reader": "^2.1.1",
    "@types/tar-fs": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "arangojs": "^8.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "kafkajs": "^2.2.4",
    "lint-staged": "^13.2.0",
    "mongoose": "^6.10.4",
    "mysql2": "^2.3.3",
    "nats": "^2.13.1",
    "neo4j-driver": "^5.6.0",
    "pg": "^8.10.0",
    "prettier": "^2.8.7",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "wait-for-expect": "^3.0.2"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "engines": {
    "node": ">= 10.16"
  }
}
